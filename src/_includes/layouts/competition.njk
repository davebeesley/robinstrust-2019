{% extends 'layouts/base.njk' %}
{% set pageType = 'Competition' %}

{# Intro content #}
{% set introHeading = title %}

{% block content %}
  <main id="main-content" tabindex="-1">
    <article class="[ post ] [ h-entry ]">
        {% include "partials/components/intro.njk" %}
        <div class="[ post__body ] [ inner-wrapper ] [ pad-top-900 pad-bottom-900 text-500 ] [ sf-flow ] [ e-content ]">
            {{ content | safe }}
            <p style="color: red;">The competition is now closed. We will let the winners know ASAP</p>
        </div>
    </article>
  </main>
{% endblock %}
